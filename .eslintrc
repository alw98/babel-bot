{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "google",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "linebreak-style": "off",
    "object-curly-spacing": "off",
    "indent": [
      "error",
      "tab"
    ],
    "@typescript-eslint/no-var-requires": "off",
    "require-jsdoc": "off",
    "no-tabs": "off",
    "max-len": [
      "error",
      120
    ],
    "no-invalid-this": "off"
  }
}